{
	"schemaVersion": 1,
	"id": "force-survival-mode",
	"version": "${version}",
	"name": "Force Survival Mode",
	"description": "A minecraft fabric mod that forces players into survival mode.",
	"authors": [
		"Neluxx"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/force-survival-mode/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"neluxx.force_survival_mode.ForceSurvivalMode"
		],
		"client": [
			"neluxx.force_survival_mode.ForceSurvivalModeClient"
		]
	},
	"mixins": [
		"force-survival-mode.mixins.json",
		{
			"config": "force-survival-mode.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.21.10",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}